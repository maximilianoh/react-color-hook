{
  "name": "react-color-hook",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --env.development --progress",
    "build": "webpack --env.production --progress",
    "test": "jest",
    "doc": "node styleguidist.js",
    "styleguide": "npx styleguidist build",
    "clean": "rm -rf lib && mkdir lib",
    "lib": "npm run clean && babel src -d lib --config-file ./buildBabel.config.js",
    "prepublish": "npm run lib"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximilianoh/react-color-hook"
  },
  "bugs": {
    "url": "https://github.com/maximilianoh/react-color-hook/issues"
  },
  "keywords": [
    "react",
    "hooks",
    "color picker",
    "react-component",
    "colorpicker",
    "picker",
    "sketch",
    "chrome",
    "photoshop",
    "material design",
    "popup"
  ],
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@loadable/component": "^5.12.0",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "babel-loader": "^8.0.6",
    "chroma-js": "^2.1.0",
    "core-js": "^3.6.4",
    "jest-canvas-mock": "^2.2.0",
    "lodash": "^4.17.15",
    "material-colors": "^1.2.6",
    "open": "^7.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "reactcss": "^1.2.3",
    "shortid": "^2.2.15",
    "webpack": "^4.41.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "react-styleguidist": "^10.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-hot-middleware": "^2.25.0"
  },
  "jest": {
    "rootDir": "./",
    "roots": [
      "<rootDir>/src"
    ],
    "verbose": false,
    "transform": {
      "^.+\\.jsx|.js?$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
