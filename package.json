{
  "name": "react-color-hook",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --env.development --progress",
    "build": "webpack --env.production --progress",
    "test": "jest",
    "doc": "node styleguidist.js",
    "styleguide": "npx styleguidist build",
    "clean": "rm -rf lib && mkdir lib",
    "lib": "npm run clean && babel src -d lib --config-file ./buildBabel.config.js",
    "prepublish": "npm run snyk-protect && npm run lib",
    "snyk-protect": "snyk protect"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximilianoh/react-color-hook"
  },
  "bugs": {
    "url": "https://github.com/maximilianoh/react-color-hook/issues"
  },
  "keywords": [
    "react",
    "hooks",
    "color picker",
    "react-component",
    "colorpicker",
    "picker",
    "sketch",
    "chrome",
    "photoshop",
    "material design",
    "popup"
  ],
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@loadable/component": "^5.13.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "babel-loader": "^8.1.0",
    "chroma-js": "^2.1.0",
    "core-js": "^3.6.5",
    "jest-canvas-mock": "^2.2.0",
    "lodash": "^4.17.20",
    "material-colors": "^1.2.6",
    "open": "^7.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "reactcss": "^1.2.3",
    "shortid": "^2.2.15",
    "snyk": "^1.385.0",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@testing-library/react": "^10.4.9",
    "babel-eslint": "^10.1.0",
    "babel-preset-minify": "^0.5.1",
    "css-loader": "^3.6.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "react-styleguidist": "^11.0.8",
    "style-loader": "^1.2.1",
    "webpack-cli": "^3.3.12",
    "webpack-hot-middleware": "^2.25.0"
  },
  "jest": {
    "rootDir": "./",
    "roots": [
      "<rootDir>/src"
    ],
    "verbose": false,
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx|.js?$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "snyk": true
}
